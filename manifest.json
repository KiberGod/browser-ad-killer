{
  "manifest_version": 3,
  "name": "YouTube Tab IDs",
  "version": "1.0",
  "description": "Extension to display tab IDs on YouTube.",
  "permissions": [
    "activeTab", 
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
